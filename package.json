{
  "name": "boomerang-game",
  "version": "1.0.0",
  "description": "回旋斩 - A fast-paced boomerang battle game built with You Engine",
  "type": "module",
  "packageManager": "pnpm@9.14.2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint --write .",
    "lint:check": "biome lint .",
    "check": "biome check --write .",
    "check:ci": "biome check ."
  },
  "dependencies": {
    "you-engine": "link:../you-engine"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "typescript": "^5.3.3",
    "vite": "^5.4.11"
  }
}
